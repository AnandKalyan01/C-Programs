// Print the 'this is github ' for n times 

#include<stdio.h>

int main(void) {
       int n ;
       printf("Enter value for n ");
       scanf("%d" , &n);
               // loop used below to do this reptitive task 
            for(int i = 0 ; i < n ; i++)
             {
                printf("this is github \n");
             }
}
