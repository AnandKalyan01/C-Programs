#include<stdio.h> // Header file

int main () {

    int a , b ;  //  Declaring the variable with data-type integer or we can chose float (decimal ) as in place of int . 
    a = 10 ;     //  Giving the values to the variable 
    b = 20 ;     //  Giving the values to the variable 
     
     // a & b are operands . 

    printf("This is the addition %d \n" , a + b) ;         // "+ operator"
    printf("This is the substraction %d \n" , b - a ) ;    // "- operator"
    printf("This is the multiplication %d \n" , a * b ) ;  // "* operator"
    printf("This is the divide %d \n" , a % b ) ;          // "% operator"

    
}
